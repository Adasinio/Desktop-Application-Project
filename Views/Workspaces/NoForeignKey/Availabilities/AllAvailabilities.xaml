<local:AllViewBase x:Class="Firma_Transport.Views.Workspaces.NoForeignKey.Availabilities.AllAvailabilities"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Firma_Transport.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <DataGrid AutoGenerateColumns="False" ItemsSource="{Binding List}">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Kod" Binding="{Binding Code}"/>
                <DataGridTextColumn Header="Poniedziałek" Binding="{Binding Monday}"/>
                <DataGridTextColumn Header="Wtorek" Binding="{Binding Tuesday}"/>
                <DataGridTextColumn Header="Środa" Binding="{Binding Wednesday}"/>
                <DataGridTextColumn Header="Czwartek" Binding="{Binding Thursday}"/>
                <DataGridTextColumn Header="Piątek" Binding="{Binding Friday}"/>
                <DataGridTextColumn Header="Sobota" Binding="{Binding Saturday}"/>
                <DataGridTextColumn Header="Niedziela" Binding="{Binding Sunday}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</local:AllViewBase>
